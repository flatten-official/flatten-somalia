{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "main": "../../scripts/frontend/start.js",
  "scripts": {
    "build": "node ../../scripts/frontend/build.js",
    "depcheck": "../../node_modules/.bin/depcheck . --ignores=\\\"eslint-*,babel*,*/webpack,prettier,*sass*,depcheck,font-awesome,formiojs\\\"",
    "lint": "../../node_modules/.bin/eslint --fix .",
    "start": "node ../../scripts/frontend/start.js",
    "deploy": "node ../../scripts/createBranchJson.js && gcloud config set project flatten-staging-271921 && gcloud builds submit",
    "auth": "gcloud auth login && gcloud ../../scripts/frontend/config set project flatten-staging-271921",
    "serve": "npx serve --single --listen 3000 ./build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "bootswatch": "^4.5.2",
    "connected-react-router": "^6.8.0",
    "formiojs": "^4.11.2",
    "history": "^4.10.1",
    "i18next": "^19.6.3",
    "i18next-browser-languagedetector": "^6.0.0",
    "immutability-helper": "^3.1.1",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.5.1",
    "leaflet": "^1.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-dom": "^16.13.1",
    "react-formio": "^4.3.0",
    "react-i18next": "^11.7.0",
    "react-leaflet": "^2.7.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "util-shared-constants": "1.0.0"
  },
  "devDependencies": {
    "branch-name": "^1.0.0",
    "depcheck": "^1.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "serve": "^11.3.2"
  }
}
