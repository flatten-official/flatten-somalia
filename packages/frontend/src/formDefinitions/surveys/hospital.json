{
  "title": "Hospital Volunteer Survey",
  "type": "form",
  "display": "form",
  "components": [
    {
      "label": "enterPhoneNumber",
      "inputMask": "99 99 99 999",
      "validate": { "required": true },
      "key": "hospitalPhoneNumber",
      "type": "phoneNumber"
    },
    {
      "label": "email",
      "placeholder": "Optional",
      "spellcheck": false,
      "key": "hospitalEmail",
      "type": "email"
    },
    {
      "label": "newPatients",
      "validate": { "required": true, "min": 0 },
      "key": "newPatients",
      "type": "number"
    },
    {
      "label": "dischargedPatients",
      "validate": { "required": true, "min": 0 },
      "key": "dischargedPatients",
      "type": "number"
    },
    {
      "label": "totalBedsOccupied",
      "validate": { "required": true, "min": 0 },
      "key": "occupiedHospitalBeds",
      "type": "number"
    },
    {
      "label": "bedsOccupiedInCriticalCare",
      "validate": { "required": true },
      "key": "occupiedCriticalCareBeds",
      "type": "number"
    },
    {
      "label": "bedAvailable",
      "validate": { "required": true, "min": 0 },
      "key": "availableBeds",
      "type": "number"
    },
    {
      "label": "patientsCurrentlyHospitalized",
      "validate": { "required": true, "min": 0 },
      "key": "hospitalizedPatients",
      "type": "number"
    },
    {
      "label": "confirmedPositiveCOVID19",
      "validate": { "required": true, "min": 0 },
      "key": "confirmedCOVID19Cases",
      "type": "number"
    },
    {
      "label": "suspectedCOVID19",
      "validate": { "required": true, "min": 0 },
      "key": "suspectedCOVID19Cases",
      "type": "number"
    },
    {
      "label": "negativeCOVID19",
      "validate": { "required": true, "min": 0 },
      "key": "negativeCOVID19Cases",
      "type": "number"
    },
    {
      "type": "button",
      "label": "submit",
      "key": "exclude-submit",
      "disableOnInvalid": true
    }
  ]
}
